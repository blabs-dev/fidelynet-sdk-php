{
  "name": "blabs/fidelynet-php",
  "description": "a wrapper for FidelyNET loyalty service API",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Salvo Bonanno",
      "email": "salvob.labs@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^7.1||^8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "graham-campbell/guzzle-factory": "^3.0||^4.0||^5.0",
    "guzzlehttp/guzzle": "^6.2||^7.0",
    "spatie/data-transfer-object": "^2.8.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5.15|^8.5|^9.3"
  },
  "autoload": {
    "psr-4": {
      "Blabs\\FidelyNet\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Blabs\\FidelyNet\\Test\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  }
}
